# Custom stuff
# Alias
alias projects='cd ~/workspaces'
alias goproject='cd ~/workspaces/golang/src/github.com/rockwotj'
alias vi='nvim'
if [ "$(uname)" == "Darwin" ]; then
  alias l='ls -CF'
  alias ls='ls -GFh'
  alias lah='ls -lah'
elif [ "$(uname -a | cut -d " " -f2)" == "raspberrypi" ]; then
  alias vi='vim'
  alias l='ls -CF --color=auto'
  alias ls='ls -GFh --color=auto'
  alias lah='ls -lah --color=auto'
elif [ "$(expr substr $(uname -s) 1 5)" == "Linux" ]; then
  alias l='ls -CF --color=auto'
  alias ls='ls -GFh --color=auto'
  alias lah='ls -lah --color=auto'
fi

